<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.ZhiHu.dao.CommentSupportDao">
    <select id="hadSupportAnswerCommentLvOne" resultType="java.lang.Long">
        select id from answer_level_one_support
        where commentId = #{commentId} and userId = #{userId}
        limit 1
    </select>
</mapper>